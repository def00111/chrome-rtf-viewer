{
  "manifest_version": 2,
  "name": "RTF Viewer",
  "description": "Display .rtf (Rich Text Format) files in the browser",
  "version": "1.2.8",
  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "<all_urls>"
  ],
  "browser_action": {},
  "background": {
    "scripts": [
      "background.js",
      "rtfRedirectHandler.js"
    ]
  },
  "incognito": "split",
  "web_accessible_resources": [
    "viewer/viewer.html"
  ]
}