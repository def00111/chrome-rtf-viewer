{
  "manifest_version": 2,
  "name": "RTF Viewer",
  "description": "Display .rtf (Rich Text Format) files in the browser",
  "version": "1.2.8",
  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "<all_urls>"
  ],
  "background": {
    "scripts": ["rtfHandler.js"]
  },
  "incognito": "split",
  "web_accessible_resources": [
    "viewer/viewer.html"
  ]
}