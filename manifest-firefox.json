{
  "manifest_version": 2,
  "name": "RTF Viewer",
  "description": "Display .rtf (Rich Text Format) files in the browser",
  "version": "1.2.6",
  "applications": {
    "gecko": {
      "strict_min_version": "56.0"
    }
  },
  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "<all_urls>"
  ],
  "background": {
    "scripts": ["rtfHandler.js"]
  },
  "incognito": "spanning",
  "web_accessible_resources": [
    "viewer/viewer.html"
  ]
}